{
    "family": "snake",
    "executionRoleArn": "arn:aws:iam::395691671709:role/CruddurServiceExecutionRole",
    "taskRoleArn": "arn:aws:iam::395691671709:role/CruddurTaskRole",
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [ 
      "FARGATE" 
    ],
    "containerDefinitions": [
      {
        "name": "snake",
        "image": "395691671709.dkr.ecr.us-east-1.amazonaws.com/snake:latest",
        "essential": true,
        "healthCheck": {
          "command": [
            "CMD-SHELL",
            "curl -f http://localhost:3000 || exit 1"
          ],
          "interval": 30,
          "timeout": 5,
          "retries": 3
        },
        "portMappings": [
          {
            "name": "snake",
            "containerPort": 3000,
            "protocol": "tcp", 
            "appProtocol": "http"
          }
        ],
  
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "snake-group",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "snake"
          }
        }
      }
    ]
  }